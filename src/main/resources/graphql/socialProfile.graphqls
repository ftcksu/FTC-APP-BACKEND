type SocialProfile {
    id: ID!
    type: String
    value:String
    account: Account
}

extend type Query {
    findAllSocialProfiles: [SocialProfile]!
}

extend type Mutation {
    newSocialProfile(type: String!,account:ID!,value:String!):SocialProfile!
    updateSocialProfileValue(value:String!,id:ID!):SocialProfile
}